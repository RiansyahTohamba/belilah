<p id="notice"><%= notice %></p>

<h1>Upload Gambars</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_gambars.each do |upload_gambar| %>
      <tr>
        <td><%= upload_gambar.title %></td>
        <td><%= upload_gambar.image_url %></td>
        <td><%= link_to 'Show', upload_gambar %></td>
        <td><%= link_to 'Edit', edit_upload_gambar_path(upload_gambar) %></td>
        <td><%= link_to 'Destroy', upload_gambar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload Gambar', new_upload_gambar_path %>
